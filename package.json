{
  "name": "jvaplaceholder",
  "version": "0.0.3-10",
  "private": true,
  "scripts": {
    "postversion": "npm run deploy",
    "clean": "npm cache clean --force && rm -rf node_modules package-lock.json && npm i --legacy-peer-deps",
    "start": "npm run watch:local",
    "watch:local": "NODE_ENV=development vite --force",
    "watch:dev": "NODE_ENV=development vite preview --mode development",
    "watch:prod": "NODE_ENV=production vite preview --mode production",
    "open:local": "node browserLaunch.js",
    "lint": "./node_modules/.bin/eslint --ignore-path .gitignore .",
    "browsers": "echo \"window.__SUPPORTED_BROWSERS__ = $(browserslist-useragent-regexp  --allowHigherVersions);\" > public/supportedBrowsers.js",
    "copy:assets": "cp -a src/assets/. dist/",
    "copy:htaccess": "cp -a src/assets/.htaccess dist/.htaccess",
    "copy:locales:watch": "cp -a src/locales serve/",
    "copy:locales:deploy": "cp -a src/locales dist/",
    "clear:dist": "rm -rf dist",
    "generate:favicons": "node generateFavicons",
    "prebuild": "npm run browsers && npm run clear:dist",
    "postbuild": "npm run copy:htaccess",
    "build": "NODE_ENV=production vite build",
    "build:dev": "npm run prebuild && NODE_ENV=development vite build --mode development",
    "build:stage": "npm run prebuild && NODE_ENV=staging vite build",
    "build:prod": "npm run prebuild && NODE_ENV=production vite build",
    "test": "node node_modules/.bin/jest --watch",
    "open:stories": "open -a \"Google Chrome Canary\" \"http://localhost:9001\"",
    "start:stories": "npm run open:stories && start-storybook -p 9001",
    "build:stories": "build-storybook -o ./stories -c .storybook -w",
    "serve:dev": "NODE_ENV=development npx env-cmd --file .env.development node ../MODULES/azt/serve/src/index.js",
    "serve:prod": "NODE_ENV=development npx env-cmd --file .env.production node ../MODULES/azt/serve/src/index.js",
    "deploy": "bash ./deploy.sh"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.21.0",
    "@vueuse/core": "^10.1.2",
    "marked": "^4.3.0",
    "ofetch": "^1.0.1",
    "pinia": "^2.0.32",
    "veaury": "^2.3.12",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "cypress": "^12.7.0",
    "eslint": "^8.34.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.9.0",
    "prettier": "^2.8.4",
    "start-server-and-test": "^2.0.0",
    "stylus": "^0.59.0",
    "stylus-loader": "^7.1.0",
    "vite": "^4.1.4",
    "vite-plugin-html": "^3.2.0"
  }
}
